        block CustomHead
    body
        nav
            div(class='nav-wrapper')
                a(href='/' class='brand-logo')
                    img(src='/images/logo.png' height='40px')
                ul(class='searchNav hide-on-med-and-down')
                    li
                        input(type='text' placeholder='지역을 검색해보세요')
                    li
                        i(class='material-icons') search
                a(href='#' data-activates='mobile_menu' class='button-collapse')
                    i(class='material-icons') menu
                ul(class='right hide-on-med-and-down')
                    li
                        a(href='/mypage')
                            div(class='profileImage' style='background: url(/images/tempProfile.jpg);')
                    li(class='nav_name') yeji Jeon
                    li
                        div(class='btn nav_button') 요청하기
                    li
                        a(href='/UploadAsk')
                            div(class='btn nav_button') 제안하기
                ul(class='side-nav' id='mobile_menu')
                    li
                        div(class='profileImage' style='background: url(/images/myProfile.png);')
                        div(class='nav_name') yeji Jeon
                    li 요청하기
                    li 제안하기
                script $(".button-collapse").sideNav();
        block content
