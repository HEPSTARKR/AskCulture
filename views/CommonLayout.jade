                div(class='googlePlusRegister')
                    div(id='googlePlusRegisterButton')
                div(class='registerDivider')
                    span or
                div(class='formDataContainer')
                    div(class='nameFormLinear')
                        input(type='text' id='firstName' placeholder='first name')
                        input(type='text' id='lastName' placeholder='last name')
                    input(type='email' id='email' placeholder='이메일 입력')
                    input(type='password' id='password' placeholder='비밀번호 입력')
                    div(class='checkPassword')
                        i(class='material-icons' id='checkDone') done
                        i(class='material-icons' id='checkFail') close
                        span 숫자/영문(대,소문자) 최소 6자리
                    input(type='password' id='passwordConfirm' placeholder='비밀번호 확인')
                    div(class='rightPassword')
                        i(class='material-icons') done
