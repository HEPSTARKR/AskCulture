doctype html
html(lang='ko')
    head
        meta(charset='utf-8')
        title AskCulture
        script(type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js")
        link(href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet")
        link(href='/stylesheets/materialize.css' media='screen,projection' type='text/css' rel='stylesheet')
        link(href='/stylesheets/materialize.min.css' media='screen,projection' type='text/css' rel='stylesheet')
        link(rel='stylesheet' type='text/css' href='/stylesheets/CommonLayout.css')
